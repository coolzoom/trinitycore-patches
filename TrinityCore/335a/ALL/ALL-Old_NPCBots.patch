+        bool MassGroupHeal(Player* player, uint32 diff)
+        {
+            if (!player->GetGroup()) return false;
+            if (IsCasting()) return false;
+            if (Rand() > 35) return false;
+
+            if (IsSpellReady(DIVINE_HYMN_1, diff, false))
