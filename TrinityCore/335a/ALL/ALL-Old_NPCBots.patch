+    //apply +healing taken
+    if (master->getLevel() >= 40)
+        RefreshAura(BOR);//+40%
+
+    if (IsMinionAI())
+    {
+        //apply -threat mod
+        switch (botOrPetType)
+        {
+            case CLASS_WARRIOR:
+            case CLASS_DEATH_KNIGHT:
