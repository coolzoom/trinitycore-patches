+            {
+                uint32 lowGUID = (*result)[0].GetUInt32();
+                uint32 entry = (*result)[4].GetUInt32();
+                Item* invItem = player->GetItemByGuid(MAKE_NEW_GUID(lowGUID, 0, HIGHGUID_ITEM));
+                if (invItem)
+                    player->_ApplyItemMods(invItem, invItem->GetSlot(), false);
