+protected:
+
+public:
+	void setUp()
+	{
+	}
+
+protected:
+	void engineMustRepeatActions()
+	{
+		MockPlayerbotAIBase mock;
+		Engine engine(&mock, new TestAiObjectContext(&mock));
+		engine.testMode = true;
+        engine.addStrategy("TestStrategy");
+        engine.Init();
+
+		for (int i=0; i<6; i++)
