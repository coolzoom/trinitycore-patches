+public:
+    void setUp()
+    {
+		EngineTestBase::setUp();
+		setupEngine(new MageAiObjectContext(ai), "arcane", NULL);
+    }
+
+protected:
+ 	void combatVsMelee()
+	{
+        tick();
+		addAura("arcane blast");
