+
+        RepeatingAction::available = FALSE;
+        engine.DoNextAction(NULL);
+        engine.DoNextAction(NULL);
+        CPPUNIT_ASSERT(AlternativeAction::executed);
+        CPPUNIT_ASSERT(PrerequisiteAction::executed);
+	}
