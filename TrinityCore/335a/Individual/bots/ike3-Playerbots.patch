+uint32 GuildTaskMgr::CreateTask(uint32 owner, uint32 guildId)
+{
+    switch (urand(0, 1))
+    {
+    case 0:
+        CreateItemTask(owner, guildId);
+        return GUILD_TASK_TYPE_ITEM;
+    default:
+        CreateKillTask(owner, guildId);
+        return GUILD_TASK_TYPE_KILL;
+    }
