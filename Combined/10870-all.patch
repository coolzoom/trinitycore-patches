+                pZarithrian = me;
+            }
+
+            void Reset()
+            {
+                instance->SetBossState(DATA_ZARITHRIAN, NOT_STARTED);
+                events.Reset();
+                summons.DespawnAll();
+                summons.clear();
+                events.ScheduleEvent(EVENT_CAST_CLEAVE_ARMOR, urand(3500,4500));
