+        }
+
+        uint32 uiIntroTimer;
+
+        uint8 uiIntroPhase;
+
+        IntroPhase Phase;
+
+        Creature* pTrall;	
+        Creature* pGarrosh;	
+        Creature* pKing;	
+        Creature* pLady;	
+        Creature* pHighlord;	
+
+        InstanceScript* pInstance;
+    	
+
+    	
+        void Reset()
+        {
+            Phase = IDLE;
+            uiIntroTimer = 0;
+            uiIntroPhase = 0;
