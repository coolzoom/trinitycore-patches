+				if (uiDamage > me->GetHealth() && (pDoneBy->GetTypeId() == TYPEID_PLAYER || pDoneBy->GetOwner()))
+				{
+					DoScriptText(NPC_ARGENT_CHAMPION_TEXT_SAY_WIN, me);
+					uiDamage = 0;
+
+					if(pDoneBy->GetOwner())
