+                            pInstance->SetData(DATA_XERESTRASZA,DONE);
+                            break;
+                    }
+                }
+            }
+
+        private:
+            bool bIntro;
+            EventMap events;
+            InstanceScript* pInstance;
+        };
+        
+        CreatureAI* GetAI(Creature *pCreature) const
+        {
+            return new npc_xerestraszaAI(pCreature);
+        }
+};
