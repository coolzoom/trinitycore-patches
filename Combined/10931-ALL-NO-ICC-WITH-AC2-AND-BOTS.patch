+		return false;
+	case MASTER_LOOT:
+		if (thisGroup->GetLooterGuid() == GetGUID())
+			return true;
+		return false;
+	case GROUP_LOOT:
+	case NEED_BEFORE_GREED:
+		if (loot->roundRobinPlayer == 0 || loot->roundRobinPlayer == GetGUID())
+			return true;
+		if (loot->hasOverThresholdItem())
+			return true;
+		return false;
+	}
+	return false;
+}
