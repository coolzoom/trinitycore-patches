+			else if(action == GOSSIP_ACTION_INFO_DEF + 9)
+				bot_class = CLASS_PRIEST;
+			else if(action == GOSSIP_ACTION_INFO_DEF + 10)
+				bot_class = CLASS_WARLOCK;
+			//else if(action == GOSSIP_ACTION_INFO_DEF + 11)
+				//bot_class = CLASS_DEATH_KNIGHT;
+
+			if(bot_class > 0)
