+    SAY_SLAY2           = -1752024,
+    SAY_DEATH           = -1752025
+};
+
+enum eSpells
+{
+    SPELL_CONFLAGRATION     = 74456,
+    SPELL_ENRAGE            = 78722,
+    SPELL_FLAME_BREATH_10   = 74403,
+    SPELL_FLAME_BREATH_25   = 74404,
+    SPELL_FLAME_BEACON      = 74453
