+void AuctionHouseBot::Initialize()
+{
+    debug_Out = sConfig->GetBoolDefault("AuctionHouseBot.DEBUG", false);
+    debug_Out_Filters = sConfig->GetBoolDefault("AuctionHouseBot.DEBUG_FILTERS", false);
