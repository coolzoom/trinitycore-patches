+		   void JustSummoned(Creature * summoned)
+		   {
+			   summons.Summon(summoned);
+		   }
+
+		   void UpdateAI(const uint32 diff)
+		   {
+			   if(!UpdateVictim())
+				   return;
+
+			   if(!isBattleActive)
+			   {
+				   summons.DespawnAll();
+				   me->DespawnOrUnsummon(1);
+				   return;
+			   }
+
+			   if(shoutBuff <= diff)
+			   {
+				   me->AddAura(SPELL_SHOUT_BUFF, me);
+				   shoutBuff = 120000; // 2 minutes
+			   }
+			   else
+				   shoutBuff -= diff;
