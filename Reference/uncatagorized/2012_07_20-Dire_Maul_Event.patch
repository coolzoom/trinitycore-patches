+		   uint32 uiPhaseChangeTimer;
+		   uint32 uiEnrageTimer;
+		   int phase;
+		   int cinematicPassed;
+
+		   void Reset()
+		   {
+			   uiCinematicTimer = 16000;
+			   uiPhaseChangeTimer = 1000;
+			   uiEnrageTimer = 5000;
+			   phase = 1;
+			   cinematicPassed = 0;
+			   me->SetFlag(UNIT_FIELD_FLAGS, UNIT_FLAG_NOT_ATTACKABLE_1 | UNIT_FLAG_OOC_NOT_ATTACKABLE | UNIT_FLAG_NOT_SELECTABLE);
