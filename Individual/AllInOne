 LevelReq.Auction = 1
 LevelReq.Mail = 1
 DungeonFinder.Enable = 0
+
+###############################################################################
+# AllInOne custom config options
+#
+#    AllInOne.OnlyGMs
+#        players are not allowed to use the NPC if this is enabled
+#        Default: 0 - disable
+#                 1 - enable
+#
+#    AllInOne.EnableAllInOneSmallBuff
+#        An option will be added to the gossip menu to Buff Players with SmallBuff
+#        Default: 1 - enable
+#                 0 - disable
+#
+#    AllInOne.EnableAllInOneGreatBuff
+#        An option will be added to the gossip menu to Buff Players with GreatBuff
+#        Default: 1 - enable
+#                 0 - disable
+#
+#    AllInOne.EnableAllInOneGMBuff
+#        An option will be added to the gossip menu to Buff GM whit GMBuff
+#        Default: 1 - enable
+#                 0 - disable
+#
+#    AllInOne.EnableTrainers
+#        An option will be added to the gossip menu for Trainers
+#        Default: 1 - enable
+#                 0 - disable
+#
+#    AllInOne.EnableMorph
+#        An option will be added to the gossip menu for Morph, 13 Famous and 13 Funny
+#        Default: 1 - enable
+#                 0 - disable
+#
+#    AllInOne.EnableBots CURRENTLY UNDER DEVELOPMENT NOT USED
+#        An option will be added to the gossip menu for the bot system
+#        Default: 1 - enable
+#                 0 - disable
+#
+#    AllInOne.EnableAllInOnePlayerTools
+#        An option will be added to the gossip menu for Player to give heal, food, mana gem
+#        Default: 1 - enable
+#                 0 - disable
+#
+#    AllInOne.EnableAllInOneGivemeGold
+#        An option will be added to the gossip menu for Player to get 5000g gold
+#        Default: 0 - disable
+#                 1 - enable
+#
+#    AllInOne.EnableAllInOneRemoveResSickness
+#        An option will be added to the gossip menu for removing resurrect sickness
+#        Default: 1 - enable
+#                 0 - disable
+#
+#    AllInOneBuffGoldCost
+#        An option will be added to the gossip menu for Buff Cost. Make sure to change the GoldCost whit the GoldCost you wanna pay.
+#        Default: 2 - GoldCost = 20000 Copper
+#
+#    AllInOne.EnableAllInOneProfessions
+#        An option will be added to the gossip menu to up your Professions
+#        Default: 1 - enable
+#                 0 - disable
+#
+#    AllInOne.EnableAllInOneSecondarySkills
+#        An option will be added to the gossip menu to up your SecondarySkills
+#        Default: 1 - enable
+#                 0 - disable
+#
+#
+#    AllInOne.EnableClassTrainers
+#        An option will be added to the gossip menu to up your ClassTrainers
+#        Default: 1 - enable
+#                 0 - disable
+#
+#    AllInOne.EnableProfessionsTrainers
+#        An option will be added to the gossip menu to up your ProfessionsTrainers
+#        Default: 1 - enable
+#                 0 - disable
+#
+#    AllInOneSkillPoints
+#        An option will be added to the gossip menu for SkillPoints you get from npc. Make sure to change the SkillPoints with the skillpoints you wanna get.
+#        Default: 5 - SkillPoints
+#
+#    AllInOneSkillGoldCost
+#        An option will be added to the gossip menu for SkillPoints Cost. Make sure to change the GoldCost whit the GoldCost you wanna pay.
+#        Default: 2 - GoldCost = 20000 Copper
+#
+###############################################################################
+
+#For GMs Only?
+AllInOne.OnlyGMs = 0
+
+#Main Menu Can be changed while server is online
+AllInOne.EnableBuffs = 1
+AllInOne.EnableProfessionsSkillUp = 1
+AllInOne.EnableTrainers = 1
+AllInOne.EnableMorph = 1
+AllInOne.EnableBots = 0 #CURRENTLY UNDER DEVELOPMENT NOT USED
+AllInOne.EnablePlayerTools = 1
+AllInOne.EnableRemoveResSickness = 1
+
+#Sub Menu Can be changed while server is online
+AllInOne.EnableSmallBuff = 1
+AllInOne.EnableGreatBuff = 1
+AllInOne.EnableGMBuff = 1
+
+#Sub Menu Can be changed while server is online
+AllInOne.EnableProfessions = 1
+AllInOne.EnableSecondarySkills = 1
+
+#Sub Menu Can be changed while server is online
+AllInOne.EnableClassTrainers = 1
+AllInOne.EnableProfessionsTrainers = 1
+
+#Sub Menu Can be changed while server is online
+AllInOne.EnableGivemeGold = 1
+
+#Cost Can be changed while server is online
+AllInOneBuffGoldCost = 20000
+AllInOneSkillGoldCost = 20000
+AllInOneSkillPoints = 5
